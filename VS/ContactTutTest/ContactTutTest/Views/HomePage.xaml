<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ContactTutTest.Views.HomePage"
             Title="HomePage">
    <StackLayout>
        
        <HorizontalStackLayout> <!--#region Top Nav-->
            <Button Text="My Profile" 
                    Background="blue"
                    x:Name="MyProfile" 
                    Clicked="MyProfile_Clicked"
                    VerticalOptions="Center">
            </Button>
        </HorizontalStackLayout>

        <ListView>
            <ListView.ItemTemplate>
                <DataTemplate DataTemplateSelector.DataTemplateContent="{Binding Name}">
                    
                </DataTemplate>
            </ListView.ItemTemplate>
        </ListView>
    </StackLayout>
</ContentPage>